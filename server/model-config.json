  "Report": {
    "dataSource": "db",
    "public": true
  },
  "Alert": {
    "dataSource": "db",
    "public": true
  },
  "React": {
    "dataSource": "db",
    "public": true
  },
  "Attention": {
    "dataSource": "db",
    "public": true
  }
